  "dependencies": {
    "@remix-run/react": "^1.3.3",
    "@remix-run/serve": "^1.3.3",
    "lru-cache": "^7.7.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "remix": "^1.3.3",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.3.3",
    "@remix-run/eslint-config": "^1.3.3",
    "@types/eslint": "^8.4.1",
    "@types/lru-cache": "^7.6.1",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "binode": "^1.0.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
